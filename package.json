    "reset:data": "npm run delete:index && npm run prepare:data",
    "clear:data": "tsx -r dotenv/config ./src/scripts/clear-pinecone-namespace.ts",
    "refill:data": "npm run clear:data && npm run prepare:data",
    "analyze:database": "tsx -r dotenv/config ./src/scripts/analyze-database.ts",
